#lang racket

;; church number
(define zero
  (lambda (f) (lambda (x) x)))

(define one
  (lambda (f) (lambda (x) (f x))))

(define two
  (lambda (f) (lambda (x) (f (f x)))))


(define (add m n)
  (lambda (f) (lambda (x) ((m f) ((n f) x)))))

(define inc
  (lambda (n) (+ 1 n)))

(((add zero one) inc) 0)

(((add two two) inc) 0)

;; another church number 

